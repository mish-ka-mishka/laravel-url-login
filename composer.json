{
    "name": "mkaverin/laravel-url-login",
    "description": "This package extends Laravelâ€™s EloquentUserProvider to support authentication with one-time secret url",
	"homepage": "https://github.com/mish-ka-mishka/laravel-url-login",
    "autoload": {
        "psr-4": {
            "UrlLogin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UrlLogin\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Michael",
            "email": "m@kaverin.ml"
        }
    ],
	"extra": {
		"laravel": {
			"providers": [
                "\\UrlLogin\\Providers\\UrlLoginServiceProvider"
			]
		}
	},
    "require": {
	    "php": "^7.4|^8.0",
	    "illuminate/contracts": "^7.0|^8.0|^9.0",
	    "illuminate/auth": "^7.0|^8.0|^9.0",
	    "illuminate/support": "^7.0|^8.0|^9.0",
	    "illuminate/validation": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.23|^7.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
